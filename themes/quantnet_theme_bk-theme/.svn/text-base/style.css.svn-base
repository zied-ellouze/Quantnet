@font-face {font-family:Helvetica;src: url(fonts/Helvetica.ttf);}
@font-face {font-family:HelveticaNeue-Italic;src: url(fonts/HelveticaNeue-Italic.otf);}
/*Reset default browser CSS.-------------------------------------------------------------- */
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, code, del, dfn, em, img, q, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, caption, tbody, tfoot, thead {
  margin: 0;
  padding: 0;
  border: 0;
  font-weight: inherit;
  font-style: inherit;
  font-size: 100%;
}
table{margin:0px; padding:0px; border-collapse:collapse;}
tr, th, td{margin:0px; padding:0px; }
/* Headings
-------------------------------------------------------------- */
h1,h2,h3,h4,h5,h6 { line-height:normal;color: #1C4164;padding: 0px 0 10px 0;}
h1{font-size: 26px; font-weight: bold;}
h2{font-size: 22px;}
h3{font-size: 20px;}
h4{font-size: 18px;}
h5{font-size: 16px;}
h6{font-size: 14px;}
/* Lists
-------------------------------------------------------------- */
ul, ol, ul li{ padding:0px; margin:0px;}
li ul,
li ol       { padding:0px; margin:0px;}
ul          { list-style-type: none; }
dl          { padding:0px; margin:0px 0px 20px 0px; }
dl dt       { font-weight: bold; }
dd          { padding:0px; margin:0px;}

/* Remove annoying border on linked images. */
a img { border: none; outline:none; }
a {outline:none; text-decoration:none; color:#212121;}
a:hover{ text-decoration:underline;}
input[type=submit]{ cursor:pointer;}
.error { color:#ff0000; font-size:12px;}
em , i{ font-style:italic; font-family: HelveticaNeue-Italic;}
pre{ font-size:12px;}
hr{ background:#E2E2E2; height:1px; margin:20px 0px; border:none;clear:both;}
/*common class*/
.clear{ clear:both;}
.alignleft ,.nav-previous{ float:left;}
.alignright , .nav-next{ float:right;}
.alignmiddle{ text-align:center;}
/* Body  ==============================================*/
body {font-size: 14px;padding:0; margin:0;color:#212121;font-family:Helvetica, Arial, sans-serif; background: #fff !important;}
